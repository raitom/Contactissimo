#modal-add-evenement.modal.fade{"aria-hidden" => "true", "aria-labelledby" => "myModalLabel", role: "dialog", tabindex: "-1"}
  .modal-dialog
    .modal-content
      .modal-header
        %button.close{"aria-hidden" => "true", "data-dismiss" => "modal", type: "button"} Ã—
        %h4#myModalLabel.modal-title Ajouter un evenement
      .modal-body
        = simple_form_for @evenement do |f|
          = f.input :nom
          = f.input :theme
          .modal-footer
            %button.btn.btn-default{"data-dismiss" => "modal", type: "button"} Fermer
            = f.button :submit, "Ajouter"
